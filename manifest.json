{
  "manifest_version": 2,
  "name": "floodlight_audit",
  "version": "0.20",
  "key": "",
  "description": "A tool that crawls a webpage and generates a floodlight tag report",
  "browser_action": {
    "default_title": "Floodlight Audit",
    "default_popup": "main.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "author": "Mauricio Desiderio",
  "permissions": [
    "activeTab",
    "identity",
    "tabs",
    "webRequest",
    "*://*/*",
    "http://*/*",
    "https://*/*",
    "cookies",
    "webNavigation",
    "debugger"
  ],
  "oauth2": {
    "client_id": "your_client_id",
    "scopes": [
      "https://www.googleapis.com/auth/dfatrafficking"
    ]
  }
}

